					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"",
							"pm.test(\"verify Body matches string\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"The Russian\");",
							"});",
							"pm.test(\"verify Content-Type is present\", function () {",
							"    pm.response.to.have.header(\"Content-Type\", \"application/json; charset=utf-8\");",
							"});",
							"pm.test(\"verify Response time is less than 200ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(10000);",
							"});",
							"",
							"var jsonData = pm.response.json();",
							"pm.test(\"verify name\", function () {",
							"    pm.expect(jsonData[1].name).to.eql(\"Just as I Am\");",
